<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:background="#FBFBFB"
    android:textAlignment="center"
    app:layout_anchorGravity="center"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="52dp"
        android:layout_height="35dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/icon" />

    <TextView
        android:id="@+id/some_id"
        android:layout_width="202dp"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:gravity="top"
        android:text="Кофейня Кобрость"
        android:textAppearance="Кофейня Кобрость"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/som1e_id"
        android:layout_width="234dp"
        android:layout_height="55dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="4dp"
        android:gravity="center_horizontal|top"
        android:text="Завтрак"
        android:textAppearance="Завтрак"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/s11ome_id"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="108dp" />

    <TextView
        android:id="@+id/s11ome_id"
        android:layout_width="223dp"
        android:layout_height="35dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="15dp"
        android:gravity="top"
        android:text="Выберите одну категорию"
        android:textAppearance="Выберите одну категорию"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/som1e_id"
        tools:layout_editor_absoluteY="143dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/cafeeee"
        tools:layout_editor_absoluteX="28dp"
        tools:layout_editor_absoluteY="182dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ddd"
        tools:layout_editor_absoluteX="265dp"
        tools:layout_editor_absoluteY="191dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/wwe"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="394dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ghhh"
        tools:layout_editor_absoluteX="260dp"
        tools:layout_editor_absoluteY="378dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="195dp"
        android:layout_height="39dp"
        android:background="#A8CAEA"
        android:gravity="center"
        android:shadowColor="#988686"
        android:text="Показать результаты"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="108dp"
        tools:layout_editor_absoluteY="584dp" />

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="57dp"
        android:background="#BBF4E3"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="673dp">

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="145dp"
            android:layout_height="26dp"
            app:srcCompat="@drawable/whatsapp" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>